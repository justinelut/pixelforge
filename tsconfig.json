{
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "preserve",
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "blocks/Image/sizes.js",
    ".next/types/**/*.ts",
    "fields/slug.js",
    "app/layout.js",
    "access/isAdmin.js",
    "utilities/formatSlug.ts",
    "payload.config.js"
, "server/index.js"  ],
  "exclude": [
    "node_modules"
  ]
}
